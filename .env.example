# TalkDub Environment Variables

# API Keys
RESEND_API_KEY=re_xxxxxxxxxxxxxxxxxxxxx
GROQ_API_KEY=gsk_xxxxxxxxxxxxxxxxxxxxx
HF_TOKEN=hf_xxxxxxxxxxxxxxxxxxxxx

# Security
SECRET_KEY=CHANGE_ME_IN_PRODUCTION_TO_RANDOM_STRING

# Cloudflare Tunnel
CLOUDFLARED_ENABLED=true
CLOUDFLARED_TOKEN=xxxxxxxxxxxxxxxxxxxxx
CLOUDFLARED_TUNNEL_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
PUBLIC_URL=https://talkdub.lab

# Redis
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# Monitoring (optional)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/xxxxxxxxxxxxx

# Environment
ENVIRONMENT=production
DEBUG=false
